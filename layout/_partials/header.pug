nav(id="nav")
    div(class="inner")
        div(class="toggle")
            div(class="lines" aria-label!=__('accessibility.nav_toggle'))
                span(class="line")
                span(class="line")
                span(class="line")
        ul(class="menu")
            li(class="item title")
                a(href=config.root rel="start")
                    != alternate || title
        ul(class="right" id="rightNav")
            li(class="item theme")
                i(class="ic i-sun")
            li(class="item search")
                i(class="ic i-search")
            != shokax_inject('rightNav')

script.
    window.addEventListener('DOMContentLoaded', () => {
        const url = 'https://cdn.jsdelivr.net/gh/ExecuteMyExecution/live2d-widget@build/autoload.js';

        if (!document.querySelector(`script[src="${url}"]`)) {
        const script = document.createElement('script');
        script.src = url;
        script.async = true;
        script.onload = () => console.log('Live2D 脚本加载完成');
        script.onerror = () => console.warn('Live2D 脚本加载失败');
        document.body.appendChild(script);
        }
    });